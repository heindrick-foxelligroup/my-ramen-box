{%- render 'navigation' with section: section -%}
{% schema %}
{
  "name": "Powerful Megamenu",
  "class": "hidden-xs flex",
  "max_blocks": 12,
  "settings": [
    {
      "type": "paragraph",
      "content": "[View the guide for this section](https://beloria-docs.boostheme.com/static-sections/powerful-megamenu-section)"
    },
    {
      "type": "header",
      "content": "Simple Menu"
    },
    {
      "type": "checkbox",
      "id": "use_link_list",
      "label": "Show a link list as menu",
      "info": "Tick the checkbox and pick a link list in below field to display a menu as a linked list instead of a mega menu",
      "default": false
    },
    {
      "type": "link_list",
      "id": "menu_link_list",
      "label": "Pick a link list"
    },
    {
      "type": "color",
      "id": "top_item_link_list_color",
      "label": "Top item color",
      "default": "#000"
    },
    {
      "type": "header",
      "content": "Dropdown background color"
    },
    {
      "type": "color",
      "id": "bkg_color_dropdown",
      "label": "Background Color",
      "default": "#fafafa"
    },
    {
      "type": "header",
      "content": "Dropdown column title"
    },
    {
      "type": "color",
      "id": "color_dropdown_title",
      "label": "Title color",
      "default": "#000"
    },
    {
      "type": "range",
      "id": "fs_dropdown_title",
      "label": "Column title's font size",
      "min": 10,
      "max": 100,
      "step": 1,
      "unit": "px",
      "default": 12
    },
    {
      "type": "select",
      "id": "align_dropdown_title",
      "label": "Align column title",
      "options": [
        {
          "value": "left",
          "label": "Left"
        },
        {
          "value": "center",
          "label": "Center"
        },
        {
          "value": "right",
          "label": "Right"
        }
      ],
      "default": "left"
    }, 
    {
      "type": "header",
      "content": "Dropdown content"
    },
    {
      "type": "color",
      "id": "color_dropdown_content",
      "label": "Text color",
      "default": "#000"
    },
    {
      "type": "range",
      "id": "fs_dropdown_content",
      "label": "Font size",
      "min": 10,
      "max": 100,
      "step": 1,
      "unit": "px",
      "default": 13
    }, 
    {
      "type": "select",
      "id": "align_dropdown_content",
      "label": "Align content",
      "options": [
        {
          "value": "left",
          "label": "Left"
        },
        {
          "value": "center",
          "label": "Center"
        },
        {
          "value": "right",
          "label": "Right"
        }
      ],
      "default": "left"
    },
    {
      "type": "header",
      "content": "Product settings"
    },
    {
      "type": "select",
      "id": "main_thumbnail",
      "label": "Show main thumbnail by",
      "options": [
        {
          "value": "product_image",
          "label": "Featured product image"
        },
        {
          "value": "variant_image",
          "label": "First available variant image"
        }
      ],
      "default": "product_image"
    },
    {
      "type": "checkbox",
      "id": "show_label",
      "label": "Show label",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "show_cart_btn",
      "label": "Show Add to cart button",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "show_qs_btn",
      "label": "Show Quick Shop button",
      "default": true
    },
    {
